<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>APP</title>
  <link href="/css/bootstrap.min.css" rel="stylesheet">
  <link rel="stylesheet" href="/css/play.css">
  <link href="/css/profile.css" rel="stylesheet">
</head>

<body>
  <div class="page-container">
    <div class="pageTitle-container">

        <div class="page-back">
          <a href="/"> &lt; Back  </a>
        </div>


      <div class="page-title">
        {{pageTitle}}
      </div>


        <div class="page-filter">
          Filter
        </div>


    </div>





<div class="pageContent-container">
  <div class="page-media">

  </div>


</div>







    <!-- SPACE FOR POPUP -->
    <div id="profile_popup" class="modal">
      </div>


    <!-- BOTTOM NAVIGATION BAR -->
    <div class="nav-container">

      <!-- Home -->
      <a href="/">
        <div class="home nav-divider">
          <div class="nav-images">
            <img src="/images/icons/home-100.png" alt="home button">
          </div>
          <div class="nav-title">
            Home
          </div>
        </div>
      </a>

      <!-- Browse -->
      <a href="/">
        <div class="browse nav-divider">
          <div class="nav-images">
            <img src="/images/icons/browse-100.png" alt="browse button">
          </div>
          <div class="nav-title">
            Browse
          </div>
        </div>
      </a>

      <!-- Profile -->
      <a href="/profile_popup.html" data-toggle="modal" data-target="#profile_popup">
        <div class="profile nav-divider">
          <div class="nav-images">
            <img src="/images/icons/profile-100.png" alt="profile button">
          </div>
          <div class="nav-title">
            Profile
          </div>

        </div>
      </a>


      <!-- Search -->
      <a href="/">
        <div class="search">
          <div class="nav-images">
            <img src="/images/icons/search-100.png" alt="search button">
          </div>
          <div class="nav-title">
            Search
          </div>
        </div>
      </a>
    </div>


  </div>

  <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
  <script src="/js/play.js"></script>
</body>

</html>
