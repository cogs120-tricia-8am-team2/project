<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>APP</title>
  <link href="/css/bootstrap.min.css" rel="stylesheet">
  <link type="text/css" rel="stylesheet" href="/css/materialize.css"  media="screen,projection"/>
  <link rel="stylesheet" href="/css/play.css">
  <link href="/css/profile.css" rel="stylesheet">
  <link rel="stylesheet" href="/css/chatroom.css">
</head>

<body>
  <div class="page-container">

    <div class="pageTitle-container">

        <div class="page-back">
          <a href="/"> &lt; Back  </a>
        </div>


      <div class="page-title">
        {{pageTitle}}
      </div>


        <div class="page-filter">
          Filter
        </div>


    </div>

    <div class="pageContent-container">
      <div class="page-media">
        <img id="arrow-left" src="/images/icons/arrow-left.svg" alt="">
        <img id="arrow-right" src="/images/icons/arrow-right.svg" alt="">
      </div>
      <div id="nav-cover-top">

      </div>

      <div class="page-media-more fixed-action-btn horizontal click-to-toggle">
        <span id="page-media-title">Media Title</span>
        <span id="page-madia-caption">Media Caption</span>
          <a id="moreBtn" class="btn-floating btn-large red">
            <i class="material-icons">+</i>
          </a>
          <ul>
            <li><a href="/play/{{pageTitle}}/MediaTitle/share" id="share-btn" class="btn-floating "><i class="material-icons">Share</i></a></li>
            <li><a class="btn-floating darken-1 "><i class="material-icons">Save</i></a></li>
            <li><a class="btn-floating "><i class="material-icons">Like</i></a></li>
            <li><a href="/play/{{pageTitle}}/MediaTitle/info" class="btn-floating "><i class="material-icons">Info</i></a></li>
          </ul>
        </div>
          <!-- SPACE FOR Chat room
          <div id="chatroom" class="chatroom-container chatroom-btns-layout">

            <div id="exit-share-btn" class="chatroom-btns">
              Exit Screen Share Mode
            </div>
            <div id="enter-btn" class="chatroom-btns chatroom-btns-last ">
              Enter Group Chatroom
            </div>

            <div id="exit-btn" class="chatroom-btns displayNone chatroom-btns-2">
              Exit
            </div>
            <div id="expand-btn" class="chatroom-btns chatroom-btns-last displayNone chatroom-btns-2">
              &circ;
            </div>
            <div id="chatweb" class="">
                <iframe src="https://www.californiabeaches.com/beach/la-jolla-shores-beach" width="100%" height="800"></iframe>
            </div>
          </div>
          -->
    </div>



    <!-- SPACE FOR POPUP -->
    <div id="profile_popup" class="modal">
      </div>

    <div class="nav-share-2 chatroom-btns-layout">
        <div id="exitChatroom" class="chatroom-btns displayNone">
          Exit Chatroom
        </div>
        <div id="up" class="chatroom-btns chatroom-btns-last displayNone">
          UP
        </div>
      </div>

    <div class="nav-share chatroom-btns-layout">
      <div id="exitSharing" class="chatroom-btns">
        Exit Sharing
      </div>
      <div id="enterChatroom" class="chatroom-btns chatroom-btns-last">
        Enter Chatroom
      </div>
    </div>


    </div>
    <!-- BOTTOM NAVIGATION BAR -->

    <div class="chat-view displayNone">

      <iframe id="myFrame" src="https://www.californiabeaches.com/beach/la-jolla-shores-beach" width="100%" height="auto"></iframe>


    </div>

    <div class="nav-container">

      <!-- Home -->
      <a href="/">
        <div class="home nav-divider">
          <div class="nav-images">
            <img src="/images/icons/home-100.png" alt="home button">
          </div>
          <div class="nav-title">
            Home
          </div>
        </div>
      </a>

      <!-- Browse -->
      <a href="/browse">
        <div class="browse nav-divider">
          <div class="nav-images">
            <img src="/images/icons/browse-100.png" alt="browse button">
          </div>
          <div class="nav-title">
            Browse
          </div>
        </div>
      </a>

      <!-- Profile -->
      <a href="/profile_popup.html" data-toggle="modal" data-target="#profile_popup">
        <div class="profile nav-divider">
          <div class="nav-images">
            <img src="/images/icons/profile-100.png" alt="profile button">
          </div>
          <div class="nav-title">
            Profile
          </div>

        </div>
      </a>

      <!-- Search -->
      <a href="/">
        <div class="search">
          <div class="nav-images">
            <img src="/images/icons/search-100.png" alt="search button">
          </div>
          <div class="nav-title">
            Search
          </div>
        </div>
      </a>
    </div>

  <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
  <script type="text/javascript" src="/js/materialize.min.js"></script>
  <script src="/js/play.js"></script>
</body>

</html>
